<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Insider - CaseStudy Automation">
    <listeners>
        <listener class-name="github.celikyunusemre.listeners.TestResultListeners"/>
    </listeners>
    <parameter name="browser" value="chrome" />
    <parameter name="url" value="https://useinsider.com" />

    <test name="Web Test">
        <classes>
            <class name="github.celikyunusemre.tests.WebTestAutomation"/>
        </classes>
    </test>
</suite>